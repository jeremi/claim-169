{
  "name": "claim169",
  "version": "0.1.0-alpha.2",
  "description": "MOSIP Claim 169 QR Code decoder library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build:wasm": "wasm-pack build ../../core/claim169-wasm --target bundler --release --out-dir ../../sdks/typescript/wasm",
    "build:ts": "tsc",
    "build": "npm run build:wasm && npm run build:ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist wasm coverage"
  },
  "keywords": [
    "mosip",
    "claim169",
    "qr-code",
    "identity",
    "cbor",
    "cose",
    "wasm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremi/claim-169"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.0",
    "typescript": "^5.7.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.0"
  }
}
